{
  "name": "@statoscope/webpack-ui",
  "version": "5.6.1",
  "description": "Webpack plugin and UI for analyzing webpack bundle with Statoscope",
  "main": "./dist/main.js",
  "types": "./types.d.ts",
  "scripts": {
    "build": "../../node_modules/.bin/webpack",
    "watch": "../../node_modules/.bin/webpack --watch",
    "build:prod": "NODE_ENV=production ../../node_modules/.bin/webpack",
    "prepublishOnly": "npm run build:prod",
    "lint:custom": "../../node_modules/.bin/tsc --noEmit"
  },
  "repository": "https://github.com/statoscope/statoscope.git",
  "author": {
    "name": "Sergey Melyukov",
    "url": "https://twitter.com/smelukov"
  },
  "files": [
    "dist/*",
    "types.d.ts"
  ],
  "publishConfig": {
    "access": "public"
  },
  "license": "MIT",
  "homepage": "https://github.com/statoscope/statoscope/",
  "dependencies": {
    "@statoscope/types": "^5.4.0"
  },
  "devDependencies": {
    "@carrotsearch/foamtree": "3.5.0",
    "@discoveryjs/discovery": "^1.0.0-beta.60",
    "@popperjs/core": "^2.9.1",
    "@statoscope/helpers": "^5.6.1",
    "@statoscope/stats": "^5.3.0",
    "@statoscope/stats-extension-compressed": "^5.6.1",
    "@statoscope/stats-extension-package-info": "^5.6.1",
    "@statoscope/webpack-model": "^5.6.1",
    "@wdxlab/events": "^1.1.0",
    "babel-loader": "^8.2.2",
    "css-loader": "^5.1.3",
    "path-browserify": "^1.0.1",
    "style-loader": "^2.0.0"
  }
}
